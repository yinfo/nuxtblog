{
  "name": "top-editor",
  "description": "A markdown editor",
  "version": "0.0.8",
  "author": "wmui",
  "private": false,
  "main": "dist/build.js",
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot --port 3005",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
